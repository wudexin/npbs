        </id>
        <property name="ecOperCode" type="java.lang.String">
            <column name="EC_OPER_CODE" length="20" not-null="true" />
        </property>
        <property name="ecAuthOper" type="java.lang.String">
            <column name="EC_AUTH_OPER" length="20" not-null="true" />
        </property>
        <property name="col1" type="java.lang.String">
            <column name="COL1" length="20" />
        </property>
        <property name="col2" type="java.lang.String">
            <column name="COL2" length="20" />
        </property>
        <property name="col3" type="java.lang.String">
            <column name="COL3" length="20" />
        </property>
        <property name="col4" type="java.lang.String">
            <column name="COL4" length="20" />
        </property>
    </class>
</hibernate-mapping>
