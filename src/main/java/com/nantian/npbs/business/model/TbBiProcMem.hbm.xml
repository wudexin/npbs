<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nantian.npbs.business.model.TbBiProcMem" table="TB_BI_PROC_MEM" >
        <id name="pid" type="java.lang.String">
            <column name="PID" length="80" />
            <generator class="assigned" />
        </id>
        <property name="busi_code" type="java.lang.String">
            <column name="BUSI_CODE" length="3" />
        </property>
        <property name="unitcode" type="java.lang.String">
            <column name="UNITCODE" length="9" />
        </property>
    <property name="c1" type="java.lang.String">
            <column name="C1" length="50" />
        </property>
          <property name="c2" type="java.lang.String">
            <column name="C2" length="50" />
        </property>
    </class>
</hibernate-mapping>
